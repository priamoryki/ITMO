S _ _ -> write _ ^ _ <
S 0 _ -> S _ > 0 >
S 1 _ -> S _ > 1 >
S o _ -> get o ^ _ <
S a _ -> get a ^ _ <

get o 0 -> zero o ^ _ <
get o 1 -> one o ^ _ <
get a 0 -> zero a ^ _ <
get a 1 -> one a ^ _ <

zero o 0 -> zero_zero o ^ _ ^
zero o 1 -> zero_one o ^ _ ^
zero a 0 -> zero_zero a ^ _ ^
zero a 1 -> zero_one a ^ _ ^

one o 0 -> zero_one o ^ _ ^
one o 1 -> one_one o ^ _ ^
one a 0 -> zero_one a ^ _ ^
one a 1 -> one_one a ^ _ ^

zero_zero o _ -> S _ > 0 >
zero_zero a _ -> S _ > 0 >

zero_one o _ -> S _ > 1 >
zero_one a _ -> S _ > 0 >

one_one o _ -> S _ > 1 >
one_one a _ -> S _ > 1 >

write _ 0 -> AC 0 ^ _ ^
write _ 1 -> AC 1 ^ _ ^
